{
  "name": "lp-ets-boilerplate-widget",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "lodash": "^4.17.14",
    "react": "^16.8.6",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "velocity-react": "^1.4.3"
  },
  "scripts": {
    "start": "npm-run-all --parallel start:css start:react",
    "build": "npm-run-all --parallel build:css build:react",
    "start:css": "postcss src/styles/app.css -o src/styles/build/index.css -w",
    "build:css": "postcss src/styles/app.css -o src/styles/build/index.css",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "tailwindcss": "^1.0.5"
  }
}
